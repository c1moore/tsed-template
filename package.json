{
  "name": "tsed-template",
  "version": "1.0.0",
  "description": "Template for creating a Ts.ED project.",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "npm run clean && ./node_modules/.bin/tsc",
    "start": "ts-node src/index.ts",
    "start:watch": "nodemon --watch src --ext ts --delay 3 --exec npm start",
    "test": "NODE_ENV=test nyc mocha",
    "test:watch": "nodemon -e js,ts --watch src --watch test --exec \"NODE_ENV=test mocha\"",
    "lint": "npx eslint src/**/*.ts src/*.ts",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/c1moore/tsed-template.git"
  },
  "keywords": [
    "typescript",
    "ts",
    "template",
    "ts.ed",
    "tsed",
    "express",
    "node",
    "nodejs"
  ],
  "author": "Calvin Moore <c1moore>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/c1moore/tsed-template/issues"
  },
  "homepage": "https://github.com/c1moore/tsed-template#readme",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@tsed/testing": "^5.45.0",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.1",
    "@types/qs": "^6.9.1",
    "@types/sinon": "^9.0.0",
    "@types/supertest": "^2.0.8",
    "@types/swagger-schema-official": "^2.0.21",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.3",
    "nyc": "^15.0.1",
    "sinon": "^9.0.2",
    "source-map-support": "^0.5.16",
    "supertest": "^4.0.2",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "reporter": [
      "lcov",
      "json",
      "text"
    ],
    "exclude": [
      "**/*.spec.ts",
      "**/*.spec.js",
      "**/dist/**",
      "**/node_modules/**",
      "**/coverage/**",
      "**/test/**"
    ],
    "excludeAfterRemap": false
  },
  "dependencies": {
    "@tsed/common": "^5.45.0",
    "@tsed/core": "^5.45.0",
    "@tsed/di": "^5.45.0",
    "@tsed/swagger": "^5.45.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "method-override": "^3.0.0",
    "qs": "^6.9.3",
    "ts-httpexceptions": "^4.1.0"
  }
}
